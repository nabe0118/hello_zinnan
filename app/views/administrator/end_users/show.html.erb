  <h2>＜ユーザー情報＞</h2>
  <table>

   <tbody>
    <tr>
      <th>名前</th>
      <td><%= @user.family_name %><%= @user.first_name %></td>
    </tr>
    <tr>
      <th>ナマエ</th>
      <td><%= @user.sei %><%= @user.mei %></td>
    </tr>
    <tr>
      <th>郵便番号１</th>
      <td><%= @user.postal_code %></td>
      <td><%= link_to 'その他の住所', administrator_addresses_index_path %></td>
    </tr>
    <tr>
      <th>住所１</th>
      <td><%= @user.address %></td>
    </tr>
      <th>電話番号</th>
      <td></td>
    </tr>
    <tr>
      <th>メールアドレス</th>
      <td><%= @user.email %></td>
    </tr>
   </tbody>
  </table>

  

  <div class="edit-unsubscribe">
    <p>
     <%= link_to '編集',edit_administrator_end_user_path %>
     <%= link_to '消去', administrator_end_user_path, method: :delete %>
    </p>
  </div>
