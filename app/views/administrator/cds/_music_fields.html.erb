<div class="nested-fields" >
  <div class="form-group">
    <div class="row">
      <div class="col-xs-3">
        <%= f.number_field :number %>
      </div>
      <div class="col-xs-3">
        <%= f.text_field :name %>
      </div>
      <div class="col-xs-3">
        <%= f.collection_select :artist_id, Artist.all, :id, :name, :prompt => true %>
      </div>
      <div class="col-xs-3">
        <%= link_to_remove_association '曲を削除', f, class: 'btn btn-danger' %>
      </div>
    </div>
  </div>
</div>
