<div class="nested-fields">
  <div class="form-group">
    <%= f.label :disk番号 %><br>
    <%= f.number_field :number %>
    <%= link_to_remove_association '削除', f, class: 'btn btn-default' %>
    <table>
      <thead>
        <tr>
          <th>曲順</th>
          <th>曲名</th>
          <th>アーティスト</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
          <div  class="music">
            <%= f.fields_for :musics do |m|  %>
              <%= render 'music_fields', f: m %>
            <% end %>
          </div>
      </tbody>
    </table>
    <div class="links">
      <%= link_to_add_association '曲を追加',f, :musics,  class: 'btn btn-default'%>
    </div>
  </div>
</div>
